{
  "expo": {
    "name": "Tree Walk",
    "slug": "village-tree-walk",
    "scheme": "village-tree-walk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0A0B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.villagetreewalk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0D0A0B"
      },
      "package": "com.example.villagetreewalk"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/BarlowSemiCondensed-Thin.otf",
            "assets/fonts/BarlowSemiCondensed-ExtraLight.otf",
            "assets/fonts/BarlowSemiCondensed-Light.otf",
            "assets/fonts/BarlowSemiCondensed-Regular.otf",
            "assets/fonts/BarlowSemiCondensed-Medium.otf",
            "assets/fonts/BarlowSemiCondensed-SemiBold.otf",
            "assets/fonts/BarlowSemiCondensed-Bold.otf",
            "assets/fonts/BarlowSemiCondensed-ExtraBold.otf",
            "assets/fonts/BarlowSemiCondensed-Black.otf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
